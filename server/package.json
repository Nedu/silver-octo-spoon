{
  "name": "nfl-rush",
  "version": "1.0.0",
  "main": "src/index.ts",
  "author": "Nedu Robert",
  "license": "MIT",
  "dependencies": {
    "@types/json2csv": "^5.0.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "json2csv": "^5.0.6",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.7.0",
    "sequelize-cli": "^6.2.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/node": "^16.11.4",
    "@types/sequelize": "^4.28.10",
    "eslint": "^8.1.0",
    "nodemon": "^2.0.14",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "clean": "rm -rf dist/ && tsc -p tsconfig.json",
    "compile": "tsc",
    "prepare-env": "npm run compile && npm run migrate && npm run seed-db",
    "build": "npm run clean && npm run compile",
    "typecheck": "tsc --p .",
    "build-typecheck": "npm run typecheck && npm run build",
    "start": "npm run build-typecheck && npm run prepare-env && node ./dist/src/index.js",
    "dev": "npm run prepare-env && ts-node-dev --respawn --pretty --transpile-only src/index.ts",
    "create-db": "./node_modules/.bin/sequelize db:create",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "create-migration": "./node_modules/.bin/sequelize migration:create --name ",
    "undo-migration": "./node_modules/.bin/sequelize db:migrate:undo",
    "clear-db": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "seed-db": "./node_modules/.bin/sequelize db:seed:all",
    "undo-seed-db": "./node_modules/.bin/sequelize db:seed:undo"
  }
}
